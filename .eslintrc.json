{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "jsx-a11y/anchor-has-content": [2, ["Anchor"]],
		"indent" : [1, "tab"],
		"no-tabs" : [0],
		"no-plusplus" : [0],
		"no-confusing-arrow": [0, {"allowParens": false}],
		"no-continue": [0],
		"no-extra-strict": [0],
		"no-param-reassign": [0],
		"padded-blocks": [0]
    }
}
